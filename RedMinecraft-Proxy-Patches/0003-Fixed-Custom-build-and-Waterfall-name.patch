diff --git a/pom.xml b/pom.xml
index 0eb69a14..e8896730 100644
--- a/pom.xml
+++ b/pom.xml
@@ -365,8 +365,8 @@
                                 </goals>
                                 <inherited>false</inherited>
                                 <configuration>
